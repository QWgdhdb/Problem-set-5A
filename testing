public class FractionTest
{
    public static void main(String[] args)
    {
        Fraction f1 = new Fraction("1/2");
        Fraction f2 = new Fraction(1, 3);
        Fraction f3 = new Fraction();
        Fraction f4 = new Fraction(f2);

        System.out.println(f1);
        System.out.println(f2);
        System.out.println(f3);
        System.out.println(f4);

        System.out.println("f1 + f2 = " + Fraction.add(f1, f2));
        System.out.println("f1 - f2 = " + Fraction.subtract(f1, f2));
        System.out.println("f1 * f2 = " + Fraction.multiply(f1, f2));
        System.out.println("f1 / f2 = " + Fraction.divide(f1, f2));

        System.out.println("f1 as double" + f1.toDouble());
        f1.setNum(2);
        f1.setDenom(4);
        System.out.println("Numerator:" + f1.getNum());
        System.out.println("Denominator:" + f1.getDenom());


    }
}
